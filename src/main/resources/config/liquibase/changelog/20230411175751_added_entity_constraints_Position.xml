    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Position.
    -->
    <changeSet id="20230411175751-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="position_id"
                                 baseTableName="rel_position__parent"
                                 constraintName="fk_rel_position__parent__position_id"
