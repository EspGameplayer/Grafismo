<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Team.
    -->
    <changeSet id="20230411175724-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="parent_id"
                                 baseTableName="team"
                                 constraintName="fk_team__parent_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>

        <addForeignKeyConstraint baseColumnNames="preferred_formation_id"
                                 baseTableName="team"
                                 constraintName="fk_team__preferred_formation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="formation"/>

        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="team"
                                 constraintName="fk_team__location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="team_id"
                                 baseTableName="rel_team__venue"
                                 constraintName="fk_rel_team__venue__team_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>

        <addForeignKeyConstraint baseColumnNames="venue_id"
                                 baseTableName="rel_team__venue"
                                 constraintName="fk_rel_team__venue__venue_id"
                                 referencedColumnNames="id"
                                 referencedTableName="venue"/>
    </changeSet>
</databaseChangeLog>
