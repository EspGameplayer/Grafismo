    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity MatchAction.
    -->
    <changeSet id="20230828205826-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="match_id"
                                 baseTableName="match_action"
                                 constraintName="fk_match_action__match_id"
