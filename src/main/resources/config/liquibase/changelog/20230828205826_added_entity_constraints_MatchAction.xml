        <addForeignKeyConstraint baseColumnNames="match_player_id"
                                 baseTableName="rel_match_action__match_player"
                                 constraintName="fk_rel_match_action__match_player__match_player_id"
                                 referencedColumnNames="id"
                                 referencedTableName="match_player"/>
    </changeSet>
</databaseChangeLog>
